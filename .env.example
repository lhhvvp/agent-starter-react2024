# Enviroment variables needed to connect to the LiveKit server.
LIVEKIT_API_KEY=<your_api_key>
LIVEKIT_API_SECRET=<your_api_secret>
LIVEKIT_URL=wss://<project-subdomain>.livekit.cloud


# Internally used environment variables
NEXT_PUBLIC_APP_CONFIG_ENDPOINT=
SANDBOX_ID=

# User management service base URL for auth proxy
# e.g. http://127.0.0.1:8081
USER_MGMT_BASE_URL=

# Development only: expose demo token returned by user management
# 1 = enabled (development only), otherwise omit or set to 0 in production
EXPOSE_DEMO_TOKEN=0

# Development only: emit additional error details to API responses and server logs
# 1 = enabled (default in non-production)
AUTH_DEBUG=1
